{% extends 'base.html.twig' %}

{% block title %}Rent{% endblock %}

{% block body %}
    <h1>Rent</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ rent.id }}</td>
            </tr>
            <tr>
                <th>RentDate</th>
                <td>{{ rent.rentDate ? rent.rentDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
            <tr>
                <th>ReturnDate</th>
                <td>{{ rent.returnDate ? rent.returnDate|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('rent_index') }}">back to list</a>

    <a href="{{ path('rent_edit', {'id': rent.id}) }}">edit</a>

    {{ include('rent/_delete_form.html.twig') }}
{% endblock %}
